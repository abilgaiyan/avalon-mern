    "debug": "^2.2.0",
    "express": "^4.16.4",
    "express-ipfilter": "^0.3.1",
    "imap": "~0.8.9",
    "localtunnel": "^1.8.3",
    "lodash": "^4.17.4",
    "mailparser": "0.6.2",
    "mongoose": "^4.13.0",
    "nodemon": "^1.12.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "path-parser": "^3.0.1",
    "react-typist": "^2.0.4",
